{
  "name": "outfit-matcher",
  "version": "1.0.0",
  "description": "AI-powered outfit suggestion and wardrobe management system",
  "private": true,
  "workspaces": [
    "server",
    "web",
    "mobile",
    "shared",
    "ai"
  ],
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:web\"",
    "start:server": "npm run start --workspace=server",
    "start:web": "npm run start --workspace=web",
    "build": "npm run build --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present"
  },
  "keywords": [
    "fashion",
    "ai",
    "wardrobe",
    "outfit",
    "style"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.0.1",
    "typescript": "^5.0.4"
  }
}
